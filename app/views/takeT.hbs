<!--Body-->

<main role="main" class="container-fluid main-dashboard">
  <section id="dashboard-jumbotron" class="container jumbotron">
    <div class="row">
      <div class="col-12 col-md-6 offset-md-3">
		  <h2 class="text-center">Your Test</h2>


        <ol id="testGenerated">


        {{#each testObj }}
        <li>{{question_phrase}}</li>
        {{#each answers}}

		<div class="form-check">
        
		<input class="form-check-input" type="radio" id="{{@index}}+{{@../index}}" name="answer{{@../index}}" value="answer">

        <label class="form-check-label" for="{{@index}}+{{@../index}}">{{answer}}

			

        </label>
        </div>
        {{/each}}
        
        {{/each}}



        

        <button class="btn btn-default" type="reset">reset</button>
        <button class="btn btn-primary" type="submit">submit</button>
      </div>
    </div>
    <!--row--> 
    
  </section>
</main>

<!--End Body -->
